// COLOR SELECT // 

         program {
            name:   "mouse_click_colorselector";
            signal: "mouse,clicked,1";
            source: "cs_icon"; 
            script {
                        new st[31];
                        new Float:vl;
                        get_state(PART:"color_swallow", st, 30, vl);

                        if (strcmp(st, "on"))
                        {
                            set_state(PART:"color_swallow", "on", 0.0);
                            emit("swallow_cs", "1");
                        }
                        if (strcmp(st, "default"))
                        {
                            set_state(PART:"color_swallow", "default", 0.0);
                            emit("unswallow_cs", "1");
                        }

                   }
            }

